{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "Prism":true
  },
  "rules": {
    "no-param-reassign": 0,
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "react/prop-types":0,
    "react/no-danger":0,
    "react/no-array-index-key":0,
    "jsx-a11y/href-no-hash": 0,
    "jsx-a11y/label-has-for":0,
    "no-underscore-dangle":0
  }
}